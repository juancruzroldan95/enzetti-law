{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@icons/*": ["./src/icons/*"],
      "@pages/*": ["./src/pages/*"],
      "@sections/*": ["./src/sections/*"],
      "@services/*": ["./src/services/*"],
      "@utils/*": ["./src/utils/*"],
    }
  }
}
